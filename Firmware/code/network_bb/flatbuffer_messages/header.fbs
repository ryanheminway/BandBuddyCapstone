namespace Server.Header;

enum Stages:ubyte {Stage1 = 0, Stage2, Stage3 = 2}

enum Cmds:ubyte {Register = 0, Stage1_data_ready, Stage2_data_ready, Stage3_data_ready, Stage1_data = 4}

table Header {
    payload_size:uint32;
    destination:Stages;
    cmd:Cmds;
    stage_id:Stages;
}

root_type Header;